{
  "presets": [
    ["@babel/preset-env", {"modules": false}],
    "@babel/preset-react"
  ],
  "plugins": ["transform-class-properties", "@babel/plugin-proposal-object-rest-spread", "react-hot-loader/babel", "syntax-dynamic-import", "react-imported-component/babel", "@babel/transform-runtime"],
  "env": {
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
      ],
      "plugins": ["transform-class-properties", "@babel/plugin-proposal-object-rest-spread", "dynamic-import-webpack"]
    }
  }
}
